<script>
    export default {
        name: "status-bar",
        data(){
          return{
            time:{
              shi:"",fen:'',miao:''
            }
          }
        },
        methods:{
          logout(){
            this.$confirm('是否退出登录?', '提示', {
              confirmButtonText: '确定',
              cancelButtonText: '取消',
              type: 'warning'
            }).then(() => {
              this.logoutOk();
            }).catch(() => {
              console.log("退出登录操作已取消")
            });

          },
          logoutOk(){
            this.axios({
              method:"post",
              url:"/logout",
              data:{},
              transformRequest: [function (data) {
                let ret = ''
                for (let it in data) {
                  ret += encodeURIComponent(it) + '=' + encodeURIComponent(data[it]) + '&'
                }
                return ret
              }],
              headers: {
                'Content-type': 'application/x-www-form-urlencoded; charset=UTF-8'
              }
            }).then((res)=> {
              if(res.data.status == "success"){
                this.$router.push({path:"/login"});
                this.$root.clearUser();
              }else{
                this.$message.error("退出失败:"+res.data.msg);
              }
            })
          }
        },
        mounted(){
          let time = "12:56:34";
          let arr = time.split(":");
          this.time = {
            shi:arr[0],fen:arr[1],miao:arr[2]
          }
        }
    }
</script>

