import Vue from 'vue'
import Router from 'vue-router'

Vue.use(Router);

export default new Router({
  routes: [
    {
      path: '/',
      name: 'index',
      component: () => import("@/components/index.vue"),
      redirect:"/home",
      children:[
        {
          path:"home",
          name:"home",
          component:() => import("@/components/home.vue"),
        },
        {
          path:"callRecords",
          name:"callRecords",
          component:() => import("@/components/callRecords.vue")
        }
      ]
    }
  ]
})
