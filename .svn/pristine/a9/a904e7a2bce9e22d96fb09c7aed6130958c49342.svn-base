      methods:{
        //获取工单列表
        getList(){
          this.list = [];
          this.axios({
            method:"post",
            url:"/report/responseTime",
            params:{...this.tjian,page:this.page,cnt:this.cnt},
          }).then((res) => {
            if(res.status == 200){
              this.total = res.data.total;
              this.list = res.data.list;
            }else{
              this.$message.error("请求失败");
            }
          })
        },
        //翻页
        changePage(page){
          this.page = page;
          this.getList();
        },
      },
